(
 (('APD () (exp ys)) ys)
 (('APD ((exp x) . (exp xs)) (exp ys)) `(,x . ,(& (APD ,xs ,ys))))

 (('MUL 0 _) 0)
 (('MUL 1 (num x)) x)
 (('MUL (num x) (num y)) (+ y (& (MUL ,(- x 1) ,y))))

 (('REV (exp xs)) (& (REV ,xs ())))
 (('REV () (exp rs)) rs)
 (('REV ((exp x) . (exp xs)) (exp rs)) (& (REV ,xs (,x . ,rs))))
