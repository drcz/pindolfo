(;(a piece of history, the very first pindolfo script)
 (('APD () ?ys) ys)
 (('APD (?x . ?xs) ?ys) `(,x . ,^(APD ,xs ,ys)))

 (('MUL  0  _) 0)
 (('MUL  1 %x) x)
 (('MUL %x %y) (+ y ^(MUL ,(- x 1) ,y)))

 (('REV ?xs) ^(REV ,xs ()))
 (('REV () ?rs) rs)
 (('REV (?x . ?xs) ?rs) ^(REV ,xs (,x . ,rs)))
)
